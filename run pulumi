go build github.com/rawdaGastan/pulumi-provider-grid

pulumi plugin install resource pulumi-provider-grid v1.0.0 -f pulumi-provider-grid --reinstall



go build -o pulumi-resource-grid github.com/rawdaGastan/pulumi-provider-grid
cd example
mkdir $PWD/state
pulumi login --cloud-url file://$PWD/state
pulumi stack init test
pulumi up --yes
pulumi up --yes
pulumi destroy --yes
pulumi stack rm --yes
pulumi logout
